completeness_year






missing_percent = (
    data_long.groupby("REF_AREA_LABEL")["Value"]
    .apply(lambda x: x.isna().mean() * 100)
    .reset_index(name="PercentMissing")
)

srednia_wartosc = (
    data_long.groupby("REF_AREA_LABEL")["Value"]
    .mean()
    .reset_index(name="Srednia")
)

